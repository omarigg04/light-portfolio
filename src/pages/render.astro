---
import PageLayout from "@/layouts/Base.astro";
// import { Image } from "astro:assets";

const meta = {
	description: "Here is where i drop all my multimedia rendered scenes.",
	title: "Video",
};
---

<PageLayout meta={meta}>
	<div class="space-y-6">
		<h1 class="title">Music </h1>
